{
  "name": "@malkab/ts-utils",
  "version": "4.1.13",
  "description": "TypeScript utilities, small functions highly reusable, for back and frontend.",
  "scope": "@malkab",
  "author": {
    "name": "Juan Pedro Perez Alcantara",
    "email": "jp.perez.alcantara@gmail.com",
    "url": "http://me.com"
  },
  "main": "dist/tsutils.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "utils",
    "typescript"
  ],
  "license": "UNLICENSED",
  "homepage": "https://github.com/owner/project#readme",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/malkab/code-samples.git"
  },
  "private": false,
  "scripts": {
    "clean": "rm -Rf build ; rm -Rf dist",
    "quick-test": "npm-run-all --parallel service:watch:quick-test:server service:build",
    "start": "npm-run-all --parallel service:watch:mocha:server service:build",
    "build": "npm run clean ; npm run service:build-lib",
    "build-with-docs": "npm run clean ; npm run service:build-lib ; npm run build-docs ; rm -Rf docs ; cp -R typedoc/build/html docs",
    "build-docs": "cd typedoc ; ./html ; cd ..",
    "service:build-lib": "webpack --config webpack-production.config.js --progress --colors",
    "service:build": "webpack --progress --colors",
    "service:watch:quick-test:server": "nodemon --inspect=0.0.0.0:9329 \"./build/quicktest.js\" --watch \"./build/quicktest.js\"",
    "service:watch:mocha:server": "nodemon --ext js --watch ./build/ --exec 'mocha --timeout 20000 ./build/mocha.js || true'"
  },
  "engines": {
    "node": "^12.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/lodash": "^4.14.162",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.10",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "lodash": "^4.17.20",
    "mocha": "^7.2.0",
    "nodemon": "^2.0.5",
    "npm-run-all": "^4.1.5",
    "rxjs": "^6.6.3",
    "terser": "^5.3.5",
    "terser-webpack-plugin": "4.2.3",
    "ts-loader": "^8.0.5",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "typedoc": "^0.19.0",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.0.0",
    "webpack-filter-warnings-plugin": "^1.2.1",
    "webpack-node-externals": "^2.5.2"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "moment": "^2.29.1"
  }
}
