{
  "name": "@malkab/ts-utils",
  "version": "8.0.1",
  "description": "TypeScript utilities, small functions highly reusable, for back and frontend.",
  "scope": "@malkab",
  "author": {
    "name": "Juan Pedro Perez Alcantara",
    "email": "jp.perez.alcantara@gmail.com",
    "url": "http://me.com"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "utils",
    "typescript"
  ],
  "license": "UNLICENSED",
  "homepage": "https://github.com/owner/project#readme",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/malkab/code-samples.git"
  },
  "private": false,
  "scripts": {
    "package-json-version": "echo 2021-08-10",
    "clean": "rm -Rf build ; rm -Rf dist",
    "quick-test": "npm-run-all --parallel service:watch:quick-test:server service:build",
    "start": "npm-run-all --parallel service:watch:mocha:server service:build",
    "build": "yarn clean ; yarn service:build-lib",
    "build-with-docs": "yarn clean ; yarn service:build-lib ; yarn build-docs ; rm -Rf docs ; cp -R typedoc/build/html docs",
    "build-docs": "cd typedoc ; ./html ; rm -Rf ../../docs ; cp -R build/html ../../docs ; cd ..",
    "publish-tar": "yarn build ; yarn pack --filename tsutils.tar",
    "publish-dev": "yarn build ; yarn publish --patch --no-git-tag-version",
    "publish-prod": "yarn build ; yarn build-docs ; yarn publish --no-git-tag-version",
    "service:build-lib": "webpack --config webpack-production.config.js --progress",
    "service:build": "webpack --progress --watch",
    "service:watch:quick-test:server": "nodemon --delay 0 --inspect=0.0.0.0:9329 \"./build/quicktest.js\" --watch \"./build/quicktest.js\"",
    "service:watch:mocha:server": "nodemon --delay 0 --ext js --watch ./build/ --exec 'mocha --timeout 20000 --max-old-space-size=4096 ./build/mocha.js || true'"
  },
  "engines": {
    "node": "^16.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.0.0",
    "@types/lodash": "^4.0.0",
    "@types/mocha": "9.0.0",
    "@types/node": "17.0.8",
    "@types/papaparse": "^5.3.1",
    "@types/webpack-env": "^1.16.2",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "lodash": "^4.0.0",
    "mocha": "^9.0.0",
    "nodemon": "^2.0.0",
    "npm-run-all": "^4.0.0",
    "rxjs": "^7.0.0",
    "terser": "^5.0.0",
    "terser-webpack-plugin": "^5.0.0",
    "ts-loader": "^9.0.0",
    "ts-node": "^10.0.0",
    "tslib": "^2.3.0",
    "typedoc": "0.22.10",
    "typescript": "^4.0.0",
    "webpack": "^5.0.0",
    "webpack-cli": "^4.0.0",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "axios": "0.24.0",
    "moment": "^2.29.1",
    "papaparse": "^5.3.1"
  }
}
